<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= render 'layouts/stylesheets' %>
  <%= javascript_include_tag :defaults %>
<%= javascript_include_tag 'public/javascripts/event_calendar.js' %>
  <%= csrf_meta_tag %>
</head>
<body>
<div class="container">
    <div class="row">
		<div class="span16 columns">
	  <%= render 'layouts/header' %>
      <section class="round">
	  	<% flash.each do |key, value| %>
			<div class="flash <%= key %>"><%= value %> </div>
		<% end %>
        <%= yield %>
      </section>
	  </div>
	  </div>
	  </div>
	  <%= render 'layouts/footer' %>
    </div>
  </body>
</html>
